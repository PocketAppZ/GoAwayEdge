<?xml version="1.0"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Title">Instalador GoAwayEdge</system:String>
    <system:String x:Key="Next">Avançar</system:String>
    <system:String x:Key="Back">Voltar</system:String>
    <system:String x:Key="Exit">Sair</system:String>
    <system:String x:Key="Yes">Sim</system:String>
    <system:String x:Key="No">Não</system:String>

    <!-- Welcome -->
    <system:String x:Key="WelcomeTitle">Boas-vindas</system:String>
    <system:String x:Key="WelcomeDescription">O instalador realizará todos os passos necessários para instalar o GoAwayEdge em seu sistema. Após a instalação, todas as chamadas do Edge serão redirecionadas para o seu navegador. Selecione a opção desejada para continuar.</system:String>
    <system:String x:Key="SettingsInstallTitle">Instalar</system:String>
    <system:String x:Key="SettingsInstallDescription">Instale e configure o GoAwayEdge em seu sistema.</system:String>
    <system:String x:Key="SettingsUninstallTitle">Desinstalar</system:String>
    <system:String x:Key="SettingsUninstallDescription">Desinstale o GoAwayEdge e restaure a funcionalidade do Edge.</system:String>
    <system:String x:Key="SettingsUninstallUseInstaller">Por favor, utilize o Instalador para desinstalar o GoAwayEdge.</system:String>

    <!-- License -->
    <system:String x:Key="LicenseTitle">Licença</system:String>
    <system:String x:Key="LicenseNote">Por favor, leia a seguinte Licença. Você deve aceitar os termos da licença antes de continuar com a instalação.</system:String>
    <system:String x:Key="LicenseAccept">Aceito esta licença.</system:String>
    <system:String x:Key="LicenseDecline">Recuso esta licença.</system:String>

    <!-- Settings -->
    <system:String x:Key="SettingsTitle">Configurações</system:String>
    <system:String x:Key="SettingsEdgeChannelTitle">Canal do Edge</system:String>
    <system:String x:Key="SettingsEdgeChannelDescription">Selecione o Canal do Microsoft Edge instalado.</system:String>
    <system:String x:Key="SettingsSearchEngineTitle">Mecanismo de Busca</system:String>
    <system:String x:Key="SettingsSearchEngineDescription">Selecione seu mecanismo de busca favorito.</system:String>
    <system:String x:Key="SettingsSearchEngineCustomItem">Personalizado</system:String>
    <system:String x:Key="SettingsCustomSearchEngineTitle">Mecanismo de Busca Personalizado</system:String>
    <system:String x:Key="SettingsCustomSearchEngineDescription">Por favor, insira a URL de consulta do mecanismo de busca.</system:String>
    <system:String x:Key="SettingsMSEdgeUninstallTitle">Remover o Microsoft Edge (Estável)</system:String>
    <system:String x:Key="SettingsMSEdgeUninstallDescription">Remova completamente o Microsoft Edge do sistema.</system:String>

    <!-- Setup in progress -->
    <system:String x:Key="SetupProgressTitle">Instalação em progresso...</system:String>
    <system:String x:Key="SetupProgressDescription">Aguarde até que a instalação seja concluída.</system:String>

    <!-- Setup finished -->
    <system:String x:Key="SetupFinishedTitle">Instalação concluída!</system:String>
    <system:String x:Key="SetupFinishedDescription">Simplesmente abra este Instalador, se desejar personalizar o GoAwayEdge novamente.</system:String>
    <system:String x:Key="UninstallFinishedTitle">Desinstalação concluída!</system:String>
    <system:String x:Key="UninstallFinishedDescription">O GoAwayEdge foi removido com sucesso do sistema.</system:String>
    <system:String x:Key="DonateButton">Doe via PayPal</system:String>

    <!-- Error Handling / Notifications -->
    <system:String x:Key="FailedInitialization">Inicialização falhou: {0}</system:String>
    <system:String x:Key="FailedInstallation">Instalação falhou: {0}</system:String>
    <system:String x:Key="FailedUninstallation">Desinstalação falhou: {0}</system:String>
    <system:String x:Key="FailedUpdate">Atualização falhou: {0}</system:String>
    <system:String x:Key="FailedInstallationGeneric">Instalação falhou! Por favor, tente novamente.</system:String>
    <system:String x:Key="FailedEdgeRemoval">A remoção do Microsoft Edge falhou! Por favor, tente novamente.</system:String>
    <system:String x:Key="FailedRegisterIFEO">Falha ao registrar IFEO para '{0}'. Por favor, tente novamente.</system:String>
    <system:String x:Key="NewUpdateAvailable">Uma nova versão do GoAwayEdge (v{0}) está disponível. Esta atualização inclui novas funcionalidades, correções de erros e garante a funcionalidade desta aplicação.</system:String>
    <system:String x:Key="NewNonIfeoUpdate">O Microsoft Edge foi atualizado e o GoAwayEdge precisa atualizar o arquivo de inicialização alternativo para garantir a funcionalidade do Edge.</system:String>
    <system:String x:Key="MissingIfeoFile">O arquivo de inicialização alternativo do Edge está faltando e deve ser copiado para garantir a funcionalidade do Edge. Copiar agora?</system:String>
    <system:String x:Key="InstallUpdate">Instalar Atualização</system:String>
    <system:String x:Key="RemindMeLater">Lembrar-me mais tarde</system:String>
    <system:String x:Key="SkipUpdate">Ignorar esta versão</system:String>
    <system:String x:Key="IfeoUpdateSuccessfulTitle">Atualização bem-sucedida</system:String>
    <system:String x:Key="IfeoUpdateSuccessfulDescription">O arquivo de inicialização alternativo do Edge foi atualizado com sucesso.</system:String>
    <system:String x:Key="IfeoCreateSuccessfulTitle">Criação bem-sucedida</system:String>
    <system:String x:Key="IfeoCreateSuccessfulDescription">O arquivo de inicialização alternativo do Edge foi criado com sucesso.</system:String>

</ResourceDictionary>
